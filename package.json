{
  "name": "remark-preset-lint-gatsby",
  "version": "1.0.0",
  "description": "Remark preset with settings for Gatsby docs",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/alexandrtovmach/remark-preset-lint-gatsby/issues"
  },
  "homepage": "https://github.com/alexandrtovmach/remark-preset-lint-gatsby#readme",
  "files": [
    "index.js",
		"src/*.js"
  ],
  "scripts": {
    "develop": "remark --rc-path ./index.js --quiet --watch ./templates/valid",
    "test": "remark --rc-path ./index.js -fq ./templates/valid"
  },
  "keywords": [
    "remark",
    "remark-preset",
    "gatsby"
  ],
  "author": {
    "name": "Alexandr Tovmach",
    "email": "alexandrtovmach@gmail.com",
    "url": "https://alexandrtovmach.com"
  },
  "license": "MIT",
  "dependencies": {
    "remark-frontmatter": "^1.3.2",
    "remark-lint": "^6.0.5",
    "remark-lint-checkbox-character-style": "^1.0.3",
    "remark-lint-checkbox-content-indent": "^1.0.3",
    "remark-lint-final-newline": "^1.0.3",
    "remark-lint-first-heading-level": "^1.1.4",
    "remark-lint-no-heading-indent": "^1.0.3",
    "remark-lint-no-tabs": "^1.0.3",
    "remark-lint-no-trailing-spaces": "^2.0.1",
    "remark-lint-prohibited-strings": "^1.2.0",
    "remark-preset-lint-markdown-style-guide": "^2.1.3"
  },
  "devDependencies": {
    "remark-cli": "^7.0.0"
  }
}
